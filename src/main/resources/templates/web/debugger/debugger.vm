<header class="aui-page-header aui-page-header-fixed header-with-border">
	<div class="aui-page-header-inner">
		<div class="aui-page-header-main">
			<h1>Velocity Debugger</h1>
		</div>
	</div>
</header>
        
<div class="aui-page-panel">
	<div class="aui-page-panel-inner">
		<section class="aui-page-panel-content">

			<h2 class="reports__category">Current Variables</h2>

			<table id="velocity-variables" border="0" cellpadding="0" cellspacing="0" class="aui">
				
				<thead>
					<tr>
						<th id="name">Name</th>
						<th id="value">Value</th>
					</tr>
				</thead>
				
				<tbody>
				
					<tr><td headers="name">loggedInUser</td><td headers="value">$loggedInUser</td></tr>
					<tr><td headers="name">project.id</td><td headers="value">$project.id</td></tr>
					<tr><td headers="name">req</td><td headers="value">$req</td></tr>
					<tr><td headers="name">params</td><td headers="value">$params</td></tr>
					<tr><td headers="name">i18n</td><td headers="value">$i18n</td></tr>
					<tr><td headers="name">hasErrorMessages</td><td headers="value">${hasErrorMessages}</td></tr>
				
				</tbody>
			</table>

		</section><!-- .aui-page-panel-content -->
	</div><!-- .aui-page-panel-inner -->
</div>
